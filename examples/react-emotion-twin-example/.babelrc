{
  "presets": [
    ["@babel/preset-env"],
    ["@babel/preset-typescript"],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "macros",
    [
      "@emotion",
      {
        "autoLabel": "always"
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    }
  }
}
