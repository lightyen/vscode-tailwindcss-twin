{
  "name": "source.tsx.csProp.twin",
  "scopeName": "source.tsx.csProp.twin",
  "injectionSelector": "L:meta.tag.attributes -comment",
  "patterns": [
    {
      "name": "meta.jsxProp.twin",
      "begin": "\\s*(cs)(=){",
      "beginCaptures": {
        "1": { "name": "entity.other.attribute-name" },
        "2": { "name": "keyword.operator.assignment" }
      },
      "end": "}",
      "patterns": [{ "include": "source.twin" }]
    },
    {
      "begin": "\\s*(cs)(=)(')",
      "beginCaptures": {
        "1": { "name": "entity.other.attribute-name" },
        "2": { "name": "keyword.operator.assignment" },
        "3": { "name": "string.quoted.single" }
      },
      "end": "(')",
      "endCaptures": {
        "1": { "name": "string.quoted.single" }
      },
      "patterns": [{ "include": "source.twin" }]
    },
    {
      "name": "meta.jsxProp.twin",
      "begin": "\\s*?(cs)(=)(\")",
      "beginCaptures": {
        "1": { "name": "entity.other.attribute-name" },
        "2": { "name": "keyword.operator.assignment" },
        "3": { "name": "string.quoted.double" }
      },
      "end": "(\")",
      "endCaptures": {
        "1": { "name": "string.quoted.double" }
      },
      "patterns": [{ "include": "source.twin" }]
    }
  ]
}
