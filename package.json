{
  "name": "tailwindcss-intellisense-twin",
  "displayName": "Tailwind Twin IntelliSense",
  "markdownDescription": "Tailwind VSCode Extension for twin.macro",
  "version": "0.0.0-tailwind3.alpha.2",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/vscode-tailwindcss-twin.git"
  },
  "publisher": "lightyen",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "autocomplete",
    "intellisense",
    "twin",
    "twin.macro",
    "css-in-js",
    "react",
    "babel-macro",
    "javascript",
    "typescript"
  ],
  "engines": {
    "vscode": "^1.56.1"
  },
  "galleryBanner": {
    "color": "#191A1F",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "activationEvents": [
    "workspaceContains:**/{*.js,*.jsx,*.ts,*.tsx}"
  ],
  "main": "./dist/extension",
  "scripts": {
    "test": "jest",
    "doit": "yarn upgrade-interactive --latest",
    "analyze": "cross-env NODE_ENV=production TS_NODE_PROJECT=webpack.tsconfig.json webpack --config webpack.analyzer.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=webpack.tsconfig.json webpack --config webpack.config.ts",
    "watch": "cross-env NODE_ENV=development TS_NODE_PROJECT=webpack.tsconfig.json webpack --config webpack.config.ts --watch",
    "vscode:prepublish": "yarn build",
    "format": "prettier --write \"**/*.{ts,json,yml}\"",
    "check-format": "prettier --check \"**/*.{ts,json,yml}\"",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@formatjs/intl": "1.14.2",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.20",
    "@types/vscode": "1.56.0",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "@typescript-eslint/typescript-estree": "5.1.0",
    "babel-loader": "8.2.2",
    "chroma-js": "2.1.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-webpack-plugin": "3.0.1",
    "fork-ts-checker-webpack-plugin": "6.3.4",
    "fuse.js": "^6.4.6",
    "jest": "27.2.5",
    "js-yaml-loader": "1.2.2",
    "prettier": "2.4.1",
    "source-map": "0.6.1",
    "tailwind-types": "3.0.0-alpha.12",
    "ts-jest": "27.0.5",
    "ts-node": "10.3.0",
    "ts-paths-resolve-plugin": "5.3.16",
    "typescript": "4.4.4",
    "typescript-paths": "1.1.2",
    "vsce": "1.100.1",
    "vscode-css-languageservice": "5.1.7",
    "vscode-languageclient": "7.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-uri": "3.0.2",
    "webpack": "5.58.1",
    "webpack-cli": "4.9.0",
    "webpack-visualizer-plugin2": "^1.0.0"
  },
  "dependencies": {
    "postcss": "8.3.9",
    "tailwindcss": "3.0.0-alpha.1"
  },
  "contributes": {
    "configurationDefaults": {
      "[javascript]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[typescript]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[javascriptreact]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[typescriptreact]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      }
    },
    "grammars": [
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.css.scss.tw",
        "path": "./syntaxes/scss.tm.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin",
        "path": "./syntaxes/twin.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-theme",
        "path": "./syntaxes/theme.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-screen",
        "path": "./syntaxes/screen.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-prop",
        "path": "./syntaxes/twProp.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.cs-prop",
        "path": "./syntaxes/csProp.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Tailwind Twin IntelliSense",
      "properties": {
        "tailwindcss.colorDecorators": {
          "markdownDescription": "%ext.settings.colorDecorators%",
          "additionalItems": false,
          "type": "string",
          "enum": [
            "on",
            "off",
            "inherit"
          ],
          "default": "inherit"
        },
        "tailwindcss.references": {
          "markdownDescription": "%ext.settings.references%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.diagnostics": {
          "markdownDescription": "%ext.settings.diagnostics%",
          "type": "object",
          "properties": {
            "enabled": {
              "markdownDescription": "%ext.settings.diagnostics.enabled%",
              "type": "boolean",
              "default": true
            },
            "conflict": {
              "type": "string",
              "enum": [
                "none",
                "loose",
                "strict"
              ],
              "default": "strict",
              "enummarkdownDescriptions": [
                "%ext.settings.diagnostics.conflict.none%",
                "%ext.settings.diagnostics.conflict.loose%",
                "%ext.settings.diagnostics.conflict.strict%"
              ]
            },
            "emptyClass": {
              "type": "boolean",
              "default": true
            },
            "emptyGroup": {
              "type": "boolean",
              "default": true
            },
            "emptyCssProperty": {
              "type": "boolean",
              "default": true
            }
          },
          "default": {
            "enabled": true,
            "conflict": "strict",
            "emptyClass": true,
            "emptyGroup": true,
            "emptyCssProperty": true
          }
        },
        "tailwindcss.preferVariantWithParentheses": {
          "type": "boolean",
          "default": false
        },
        "tailwindcss.fallbackDefaultConfig": {
          "type": "boolean",
          "default": true
        },
        "tailwindcss.enabled": {
          "markdownDescription": "%ext.settings.enabled%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.jsxPropImportChecking": {
          "markdownDescription": "%ext.settings.jsxPropImportChecking%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.rootFontSize": {
          "type": "number",
          "default": 16
        },
        "tailwindcss.logLevel": {
          "type": "string",
          "description": "%ext.settings.logging.level%",
          "enum": [
            "none",
            "error",
            "warning",
            "info",
            "debug",
            "trace"
          ],
          "default": "info"
        },
        "tailwindcss.documentColors": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "commands": []
  }
}
