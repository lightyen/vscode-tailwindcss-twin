{
  "name": "tailwindcss-intellisense-twin",
  "displayName": "Tailwind Twin IntelliSense",
  "markdownDescription": "Tailwind VSCode Extension for twin.macro",
  "version": "0.12.3",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/vscode-tailwindcss-twin.git"
  },
  "publisher": "lightyen",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "autocomplete",
    "intellisense",
    "twin",
    "twin.macro",
    "css-in-js",
    "react",
    "babel-macro",
    "javascript",
    "typescript"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "galleryBanner": {
    "color": "#191A1F",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "activationEvents": [
    "workspaceContains:**/{*.js,*.jsx,*.ts,*.tsx}"
  ],
  "main": "./dist/extension",
  "scripts": {
    "test": "jest",
    "doit": "yarn upgrade-interactive --latest",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack.analyzer.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
    "vscode:prepublish": "yarn build",
    "format": "prettier --write \"**/*.{ts,json,yml}\"",
    "check-format": "prettier --check \"**/*.{ts,json,yml}\"",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@formatjs/intl": "2.1.1",
    "@swc-node/register": "1.4.2",
    "@swc/core": "1.2.161",
    "@swc/jest": "0.2.20",
    "@types/chroma-js": "2.1.3",
    "@types/jest": "27.4.1",
    "@types/js-beautify": "1.13.3",
    "@types/node": "14.17.32",
    "@types/source-map-support": "0.5.4",
    "@types/vscode": "1.60.0",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@typescript-eslint/typescript-estree": "5.17.0",
    "@yarnpkg/pnp": "3.2.0",
    "autoprefixer": "10.4.4",
    "chroma-js": "2.4.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "esbuild": "0.14.28",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-webpack-plugin": "3.1.1",
    "fork-ts-checker-webpack-plugin": "6.4.0",
    "fuse.js": "6.5.3",
    "jest": "27.5.1",
    "js-beautify": "1.14.1",
    "js-yaml-loader": "1.2.2",
    "prettier": "2.6.1",
    "source-map-support": "0.5.21",
    "swc-loader": "0.1.15",
    "tailwind-types": "2.2.14",
    "terser-webpack-plugin": "5.3.1",
    "ts-paths-resolve-plugin": "5.6.0",
    "typescript": "4.6.3",
    "typescript-paths": "1.3.1",
    "vsce": "2.7.0",
    "vscode-css-languageservice": "5.1.8",
    "vscode-languageclient": "7.0.0",
    "vscode-languageserver": "7.0.0",
    "vscode-uri": "3.0.3",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0",
    "webpack-visualizer-plugin2": "1.0.0"
  },
  "dependencies": {
    "postcss": "8.4.12",
    "tailwindcss": "2.2.19"
  },
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.css.scss.tw",
        "path": "./syntaxes/scss.tm.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin",
        "path": "./syntaxes/twin.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-theme",
        "path": "./syntaxes/theme.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-screen",
        "path": "./syntaxes/screen.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-prop",
        "path": "./syntaxes/twProp.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.cs-prop",
        "path": "./syntaxes/csProp.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Tailwind Twin IntelliSense",
      "properties": {
        "tailwindcss.colorDecorators": {
          "markdownDescription": "%ext.settings.colorDecorators%",
          "additionalItems": false,
          "type": "string",
          "enum": [
            "on",
            "off",
            "inherit"
          ],
          "default": "inherit"
        },
        "tailwindcss.references": {
          "markdownDescription": "%ext.settings.references%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.diagnostics": {
          "markdownDescription": "%ext.settings.diagnostics%",
          "type": "object",
          "properties": {
            "enabled": {
              "markdownDescription": "%ext.settings.diagnostics.enabled%",
              "type": "boolean",
              "default": true
            },
            "conflict": {
              "type": "string",
              "enum": [
                "none",
                "loose",
                "strict"
              ],
              "default": "strict",
              "enummarkdownDescriptions": [
                "%ext.settings.diagnostics.conflict.none%",
                "%ext.settings.diagnostics.conflict.loose%",
                "%ext.settings.diagnostics.conflict.strict%"
              ]
            },
            "emptyClass": {
              "type": "boolean",
              "default": true
            },
            "emptyGroup": {
              "type": "boolean",
              "default": true
            },
            "emptyCssProperty": {
              "type": "boolean",
              "default": true
            }
          },
          "default": {
            "enabled": true,
            "conflict": "strict",
            "emptyClass": true,
            "emptyGroup": true,
            "emptyCssProperty": true
          }
        },
        "tailwindcss.preferVariantWithParentheses": {
          "type": "boolean",
          "default": false
        },
        "tailwindcss.fallbackDefaultConfig": {
          "type": "boolean",
          "default": true
        },
        "tailwindcss.enabled": {
          "markdownDescription": "%ext.settings.enabled%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.jsxPropImportChecking": {
          "markdownDescription": "%ext.settings.jsxPropImportChecking%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.rootFontSize": {
          "type": "number",
          "default": 16
        },
        "tailwindcss.hoverColorHint": {
          "type": "string",
          "enum": [
            "none",
            "hex",
            "rgb",
            "hsl"
          ],
          "default": "none"
        },
        "tailwindcss.logLevel": {
          "type": "string",
          "description": "%ext.settings.logging.level%",
          "enum": [
            "none",
            "error",
            "warning",
            "info",
            "debug",
            "trace"
          ],
          "default": "info"
        },
        "tailwindcss.documentColors": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "commands": []
  },
  "packageManager": "yarn@3.2.0-rc.1"
}
